msgid "Create Payment Groups"
msgstr "Crear grupos de pago"

msgctxt "model:ir.message,text:bank_account_not_defined"
msgid "The bank account of the company %(company)s is not defined."
msgstr "La cuenta bancaria de la empresa %(company)s no está definida."

msgctxt "model:ir.message,text:company_without_complete_address"
msgid "The company %(company)s has no a complete address to add to the file."
msgstr ""
"La empresa %(company)s no tiene una dirección completa para añadir al "
"archivo."

msgctxt "model:ir.message,text:configuration_error"
msgid "Configuration Error!"
msgstr "¡Error de configuración!"

msgctxt "model:ir.message,text:customer_bank_account_not_defined"
msgid ""
"The bank account number of the party %(party)s is not defined and current "
"payment  journal enforces all lines to have a bank account."
msgstr ""
"El número de cuenta bancaria del tercero %(party)s no está definido y el "
"diario de pago actual requiere que todas las líneas deben tener una cuenta "
"bancaria."

msgctxt "model:ir.message,text:different_process_method"
msgid ""
"Payment process method can not be mixed on payment groups. Payment process "
"\"%(process)s\" of line \"%(payment)s\" is diferent from previous payment "
"process \"%(pprocess)s\""
msgstr ""
"El método del proceso de pago no se puede mezclar en grupos de pago. El "
"proceso de pago \"%(process)s\" de la línea \"%(payment)s\" es diferente del"
" proceso de pago anterior \"%(pprocess)s\""

msgctxt "model:ir.message,text:no_lines"
msgid "Can not generate export file, there are not payment lines."
msgstr "No se puede generar el archivo de exportación, no hay líneas de pago."

msgctxt "model:ir.message,text:non_posted_move"
msgid "You can not pay line \"%(line)s\" because its move \"%(move)s\" is not posted."
msgstr ""
"No puede pagar la línea \"%(line)s\" porque su asiento \"%(move)s\" no está "
"contabilizado.'"

msgctxt "model:ir.message,text:party_without_address"
msgid "The party %(party)s has no any address to add to the file."
msgstr "El tercero %(party)s no tiene una dirección para añadir al archivo."

msgctxt "model:ir.message,text:party_without_bank_account"
msgid "The party \"%(party)s\" doesn\\'t have bank account."
msgstr "El tercero \"%(party)s\" no tiene cuenta bancaria."

msgctxt "model:ir.message,text:party_without_complete_address"
msgid "The party %(party)s has no a complete address to build the file."
msgstr ""
"El tercero %(party)s no tiene una dirección completa para construir el "
"archivo."

msgctxt "model:ir.message,text:party_without_province"
msgid "The party %(party)s has no any province assigned at its address"
msgstr ""
"El tercero %(party)s no tiene ninguna provincia asignada en su dirección"

msgctxt "model:ir.message,text:party_without_vat_code"
msgid "The party %(party)s has no any vat number."
msgstr "El tercero %(party)s no tiene ningún número vat."

msgctxt "model:ir.message,text:payment_without_bank_account"
msgid "The payment \"%(payment)s\" doesn\\'t have bank account."
msgstr "El pago \"%(payment)s\" no té compte bancari."

msgctxt "model:ir.message,text:remittance"
msgid "remittance"
msgstr "remesa"

msgctxt "model:ir.message,text:unknown_error"
msgid "Unknown error. An error occurred creating the file."
msgstr "Error desconocido. Se ha producido un error al crear el archivo."

msgctxt "model:ir.message,text:vat_code_not_defined"
msgid "The company \"%(company)s\" have not any VAT number defined."
msgstr "La empresa \"%(company)s\" no tiene ningún número VAT definido."

msgctxt "model:ir.message,text:wrong_bank_account"
msgid "The bank account number of the company %(company)s is not correct."
msgstr ""
"El número de cuenta bancaria de la empresa %(company)s no es correcto."

msgctxt "model:ir.message,text:wrong_party_bank_account"
msgid "The bank account number of the party %(party)s is not correct."
msgstr "El número de cuenta bancaria del tercero %(party)s no es correcto."

msgctxt "model:ir.message,text:wrong_payment_journal"
msgid "The payment journal has no norm to build a file."
msgstr "El diario de pago no tiene ninguna norma para construir un archivo."

msgctxt "view:account.move.line.create_payment_group.start:"
msgid "Create Payment Group"
msgstr "Crear grupos de pago"

msgctxt "view:account.payment.process.start:"
msgid ""
"If the planned date is set, the data field of all the payments will be "
"updated with this date before creating the payment group."
msgstr ""
"Si se introduce la fecha de planificación, el campo fecha de todos los pagos"
" se actualizará con esta fecha antes de crear el grupo de pagos."

