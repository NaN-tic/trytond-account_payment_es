msgctxt "help:account.move.line.create_payment_group.start,join:"
msgid "Join payment lines of the same bank account."
msgstr "Une líneas de pago de la misma cuenta bancaria."

msgctxt "help:account.move.line.create_payment_group.start,planned_date:"
msgid "Date when the payment entity must process the payment group."
msgstr "Fecha en la que la entidad debe procesar el grupo de pagos."

msgctxt "help:account.payment.journal,require_bank_account:"
msgid ""
"If your bank allows you to send payment groups without the bank account "
"info, you may disable this option."
msgstr ""
"Si su banco le permite enviar grupos de pagos sin la información de la "
"cuenta bancaria, puede desactivar esta opción."

