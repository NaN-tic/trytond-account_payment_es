msgstr "Fecha de planificación"

msgctxt "field:account.payment.journal,ine_code:"
msgid "INE code"
msgstr "Código INE"

msgctxt "field:account.payment.journal,require_bank_account:"
msgid "Require bank account"
msgstr "Requiere cuenta bancaria"

msgctxt "field:account.payment.journal,suffix:"
msgid "Suffix"
msgstr "Sufijo"

msgctxt "field:account.payment.process.start,join:"
msgid "Join lines"
msgstr "Juntar líneas"

msgctxt "field:account.payment.process.start,planned_date:"
msgid "Planned Date"
msgstr "Fecha de planificación"

msgctxt "help:account.payment.journal,require_bank_account:"
msgid ""
"If your bank allows you to send orders without the bank account info, you "
"may disable this option."
msgstr ""
"Si su banco le permite enviar ordenes de pago sin la información de la "
"cuenta bancaria, puede desactivar esta opción."

msgctxt "help:account.payment.process.start,join:"
msgid "Join payment lines of the same bank account."
msgstr "Une líneas de pago de la misma cuenta bancaria."

msgctxt "help:account.payment.process.start,planned_date:"
msgid "Date when the payment entity must process the payment order."
msgstr "Fecha en la que la entidad debe procesar la orden de pago."
