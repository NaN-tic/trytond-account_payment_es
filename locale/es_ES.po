msgctxt "error:account.payment.group:"
msgid "Can not generate export file, there are not payment lines."
msgstr "No se puede generar fichero de exportación, no hay líneas de pago."

msgctxt "error:account.payment.group:"
msgid "Configuration Error!"
msgstr "¡Error de configuración!"

msgctxt "error:account.payment.group:"
msgid "The bank account number of the company %s is not correct."
msgstr "El número de cuenta bancaria de la compañía %s no es correcto."

msgctxt "error:account.payment.group:"
msgid "The bank account number of the party %s is not correct."
msgstr "El número de cuenta bancaria del tercero %s no es correcto."

msgctxt "error:account.payment.group:"
msgid ""
"The bank account number of the party %s is not defined and current payment "
"journal enforces all lines to have a bank account."
msgstr ""
"El número de cuenta bancaria del tercero %s no esta definido y el diario de "
"pago actual fuerza que todas la líneas tenga una."

msgctxt "error:account.payment.group:"
msgid "The bank account of the company %s is not defined."
msgstr "No está definida la cuenta bancaria de la compañía %s."

msgctxt "error:account.payment.group:"
msgid "The company %s has no a complete address to add to the file."
msgstr ""
"La compañía %s no tiene una dirección completa para añadir al fichero."

msgctxt "error:account.payment.group:"
msgid "The company have not any VAT number defined."
msgstr "La compañía no tiene un CIF/NIF definido."

msgctxt "error:account.payment.group:"
msgid "The party %s has no any province assigned at its address"
msgstr "El tercero %s no tiene provincia asignada en su dirección."

msgctxt "error:account.payment.group:"
msgid "The party %s has no any vat number."
msgstr "El tercer %s no tiene CIF/NIF."

msgctxt "error:account.payment.group:"
msgid "The payment journal has no norm to build a file."
msgstr "El diario de pago no tiene una norma para construir un fichero."

msgctxt "error:account.payment.group:"
msgid "Unknown error. An error occurred creating the file."
msgstr "Error desconocido. Ha ocurrido un error creando el fichero."

msgctxt "error:account.payment.group:"
msgid "remittance"
msgstr "remesa"

msgctxt "field:account.payment.group,join:"
