msgctxt "error:account.move.line.create_payment_group.start:"
msgid ""
"Payment types can not be mixed on payment groups. Payment Type \"%s\" of "
"line \"%s\" is diferent from previous payment types \"%s\""
msgstr ""
"No es poden barrejar tipus de pagament en un sol grup de pgaments. El tipus "
"de pagament \"%s\" de la línia \"%s\" es diferent del tipus de pagament "
"utlitzat anteriorment \"%s\"."

msgctxt "error:account.payment.group:"
msgid "Can not generate export file, there are not payment lines."
msgstr ""
"No es pot generar el fitxer d'exportació, no hi han línies de pagament."

msgctxt "error:account.payment.group:"
msgid "Configuration Error!"
msgstr "Error de configuració."

msgctxt "error:account.payment.group:"
msgid "The bank account number of the company %s is not correct."
msgstr "El número de compte bancari de l'empresa %s no és correcte."

msgctxt "error:account.payment.group:"
msgid "The bank account number of the party %s is not correct."
msgstr "El número de compte bancari del tercer %s no és correcte."

msgctxt "error:account.payment.group:"
msgid ""
"The bank account number of the party %s is not defined and current payment "
"journal enforces all lines to have a bank account."
msgstr ""
"El número de compte bancari del tercer %s no està definit i l'actual diari "
"de pagament força a que totes les línies tingun un."

msgctxt "error:account.payment.group:"
msgid "The bank account of the company %s is not defined."
msgstr "No s'ha definit el número de compte bancari de l'empresa %s."

msgctxt "error:account.payment.group:"
msgid "The company %s has no a complete address to add to the file."
msgstr "L'empresa %s no té una adreça complerta per afegir al fitxer."

msgctxt "error:account.payment.group:"
msgid "The company have not any VAT number defined."
msgstr "L'empresa not té cap CIF/NIF definit."

msgctxt "error:account.payment.group:"
msgid "The party \"%s\" doesn't have bank account."
msgstr "El tercer \"%s\" no té compte bancari."

msgctxt "error:account.payment.group:"
msgid "The party %s has no any province assigned at its address"
msgstr "El tercer %s no té una província assignada a la seva adreça."

msgctxt "error:account.payment.group:"
msgid "The party %s has no any vat number."
msgstr "El tercer %s no té cap CIF/NIF."

msgctxt "error:account.payment.group:"
msgid "The payment \"%s\" doesn't have bank account."
msgstr "El pagament \"%s\" no té compte bancari."

msgctxt "error:account.payment.group:"
msgid "The payment journal has no norm to build a file."
msgstr "El diari de pagament no té una norma per construir el fitxer."

msgctxt "error:account.payment.group:"
msgid "Unknown error. An error occurred creating the file."
msgstr "Error desconegut. S'ha produït un error creant el fitxer."

msgctxt "error:account.payment.group:"
msgid "remittance"
msgstr "remesa"

